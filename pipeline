pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
               echo 'Hello' 
            }
            steps {
               echo 'Hello again' 
            }
        }
        stage('Stage 2') {
            steps {
               echo 'I am in stage 2 now' 
            }
            steps {
               echo 'Hello from stage 2' 
            }
        }
    }
}
