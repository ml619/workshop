pipeline {
    agent any 
    stages {
        stage('Stage 1') {
            steps {
               echo 'Hello'
               step('Step 1'){
                    powershell: 'echo Hello'
                    powershell: script: 'git clone https://github.com/ml619/workshop.git' 
                    powershell: script: 'python workshop/hello.py'
               }
                
            }
        }
    }
}
